{
  "name": "@fabio-arsenal/module-adaptor-testing",
  "version": "1.0.2",
  "description": "Import ES6 Modules into a Razzle Project (testing phase)",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "clean": "lerna clean --yes",
    "format": "prettier --write 'packages/*/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx)' 'packages/*/!(node_modules)/**/*.js'",
    "build-docs": "node ./scripts/build-docs.mjs",
    "prepublish": "webpack --config webpack.config.js",
    "npm-publish": "npm publish --tag latest --registry=https://registry.npmjs.org/ --access public",
    "publish-all": "node ./scripts/publish-all.mjs",
    "publish-all-stable": "yarn publish-all -c -t --push && release && node ./scripts/release-notes.js",
    "publish-all-canary": "yarn publish-all -p canary -c -t --push && release --pre",
    "publish-all-alpha": "yarn publish-all -p alpha -c -t --push && release --pre",
    "pre-publish-all": "yarn build-docs && yarn update-examples",
    "publish-to-npm": "node ./scripts/publish-to-npm.mjs",
    "git-reset": "git reset --hard HEAD",
    "git-clean": "git clean -d -x -e node_modules -e packages -f"
  },
  "repository": "git+https://github.com/fabio-arsenal/module-adaptor-testing.git",
  "private": true,
  "bugs": {
    "url": "https://github.com/fabio-arsenal/module-adaptor-testing/issues"
  },
  "homepage": "https://github.com/fabio-arsenal/module-adaptor-testing#readme",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.0",
    "@types/node": ">=18",
    "babel-loader": "^9.2.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "inquirer": "^12.3.0",
    "lerna": "^8.1.9",
    "release": "^6.3.1",
    "semver": "^7.6.3",
    "sinon": "^12.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "yargs": "^17.7.2"
  },
  "author": "fabio <fabio@arsenalbio.com>",
  "license": "MIT",
  "engines": {
    "node": "20.5.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
